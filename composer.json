{
    "name": "ssolweb/laravel-br-formatter",
    "description": "Pacote Laravel para casts e validações de padrões brasileiros como CPF, CNPJ e telefone.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "formatter",
        "cpf",
        "cnpj",
        "telefone",
        "validation",
        "casts",
        "brasil"
    ],
    "authors": [
        {
            "name": "Vinicius de Santana",
            "email": "vinicius.vts@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "SSolWEB\\LaravelBrFormatter\\LaravelBrFormatterServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "SSolWEB\\LaravelBrFormatter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SSolWEB\\LaravelBrFormatter\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.0",
        "illuminate/database": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "ssolweb/string-morpher": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5",
        "squizlabs/php_codesniffer": "3.*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
